{
  "appenders": {
    "console": {
      "type": "console"
    },
    "con-log": {
      "type": "console",
      "filename": "${opts:base}/logs/con-log-${opts:serverId}.log",
      "pattern": "connector",
      "maxLogSize": 1048576,
      "layout": {
        "type": "coloured"
      },
      "backups": 5
    },
    "rpc-log": {
      "type": "console",
      "filename": "${opts:base}/logs/rpc-log-${opts:serverId}.log",
